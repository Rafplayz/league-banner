{
  "compilerOptions": {
    "target": "esnext",                                   
    "module": "ES2020",                                 
    "moduleResolution": "node",
    "paths": {
      "discord.js":["./node_modules/discord.js"]
    },
    "allowJs": true,
    "outDir": "./js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  }
}